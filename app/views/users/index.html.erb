<ul>
  <% @results.each do |result| %>
    <% if result.searchable_type == 'User' %>
      <%= link_to user_path(result.searchable_id) do %>
        <li>
          <%= result.content %>
        </li>
      <% end %>
    <% else %>
      <%= link_to podcast_path(result.searchable_id) do %>
        <li>
          <%= result.content %>
        </li>
      <% end %>
    <% end %>
  <% end %>
</ul>
